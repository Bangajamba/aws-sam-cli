{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Docker Launch (console)",
      "type": "coreclr",
      "request": "launch",
      // "preLaunchTask": "buildForDebug",
      "program": "/var/runtime/MockBootstraps.dll",
      "args": ["test::test.Function::FunctionHandler", "{\"foo\":\"bar\"}"],
      "cwd": "/var/task",
      "sourceFileMap": {
        "/var/task": "${workspaceRoot}"
      },
      "pipeTransport": {
        "pipeProgram": "docker",
        "pipeCwd": "${workspaceRoot}",
        "pipeArgs": [ "exec", "-i", "lambda_dotnetcore2.1" ],
        "quoteArgs": false,
        "debuggerPath": "/vsdbg/vsdbg"
      },
      "justMyCode": true
    }
  ]
}